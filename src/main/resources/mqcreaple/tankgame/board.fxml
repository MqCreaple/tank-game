<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.layout.HBox?>
<?import java.lang.String?>
<?import javafx.scene.layout.AnchorPane?>
<?import java.lang.Double?>
<HBox xmlns:fx="http://javafx.com/fxml" fx:controller="mqcreaple.tankgame.BoardController" fx:id="masterPane"
      prefWidth="800.0" prefHeight="600.0">
    <fx:define>
        <String fx:id="version" fx:value="1.0" />
    </fx:define>

    <AnchorPane fx:id="gamePane" style="-fx-background-color: rgb(0, 0, 0)"
                prefHeight="${masterPane.prefHeight}" prefWidth="${gamePane.prefHeight}" />
    <VBox fx:id="statusBar" prefHeight="${masterPane.prefHeight}">
        <Label>Tom Geng's Tank Game</Label>
        <HBox>
            <Label fx:id="fps">fps: </Label>
        </HBox>
    </VBox>
</HBox>